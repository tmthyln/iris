{
  "name": "scaffold",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --env staging & vite",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc",
    "typegen": "wrangler types --env staging",
    "generate:assets": "pwa-assets-generator --preset minimal public/logo.svg",
    "build": "vite build",
    "deploy-staging": "vite build && wrangler deploy --env staging",
    "deploy-prod": "vite build && wrangler deploy --env prod"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "bulma": "^1.0.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250109.0",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "fast-xml-parser": "^4.5.1",
    "hono": "^4.6.16",
    "sass": "^1.85.0",
    "typescript": "^5.3.3",
    "vite": "^6.3.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.21.1",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vitest": "^1.2.1",
    "vue-tsc": "^1.8.25",
    "wrangler": "^3.101.0"
  }
}
